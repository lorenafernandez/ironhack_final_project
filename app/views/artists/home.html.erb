<%= render '/artists/private_header' %>
<% if  !@work.nil? && @work.new_record? %>
	<%= render '/works/form' %>
<% else %>
	<% if current_user.name.blank? %>
		<%= render '/layouts/first_time_you_come_in' %>
	<% elsif  current_user.artist.you_are.present?%>
		<%= render '/artists/show_artist_information' %>
	<% end %>
<% end %>